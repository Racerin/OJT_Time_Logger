{% extends "layout.jinja-html" %}
{% block title %}Login{% endblock %}

{% block body %}
<h2>Login</h2>
<form action="{{ url_for('auth.login') }}">
    <label for="login_id">Username/E-mail</label><br>
    <input id="login_id" type="text"><br><br>
    <label for="pw">Password</label><br>
    <input id="pw" type="password"><br><br>
    <input type="submit">
</form>
{% endblock %}